{
  "name": "ytkit",
  "description": "Youtube downloader",
  "version": "1.0.10",
  "author": "Benjamin Maggi @maggiben",
  "bin": {
    "test": "./scripts/test.js",
    "ytkit": "./bin/run"
  },
  "bugs": "https://github.com/maggiben/ytkit/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@salesforce/ts-types": "^1.5.17",
    "cli-ux": "^5.6.2",
    "sanitize-filename": "^1.6.3",
    "streamspeed": "^2.0.1",
    "tslib": "^1",
    "ytdl-core": "^4.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-command-snapshot": "^2.2.2",
    "@oclif/test": "^1.2.8",
    "@salesforce/dev-config": "^2.1.2",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.18",
    "@types/chai": "^4",
    "@types/cli-progress": "^3.9.1",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chai": "^4",
    "chalk": "^4.1.1",
    "eslint": "^7.30.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "globby": "^10",
    "husky": "^7.0.0",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "shelljs": "^0.8.4",
    "ts-node": "^10.0.0",
    "typescript": "^3.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/maggiben/ytkit",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ytkit",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "devPlugins": [
      "@oclif/plugin-command-snapshot"
    ],
    "topics": {
      "download": {
        "description": "Downloads."
      }
    }
  },
  "repository": "maggiben/ytkit",
  "scripts": {
    "build": "node ./scripts/build.js",
    "clean": "node ./scripts/clean.js",
    "compile": "node ./scripts/compile.js",
    "compile-test": "node ./scripts/compile-test.js",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "node ./scripts/lint.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "lib/index.d.ts"
}
